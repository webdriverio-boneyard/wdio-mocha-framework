{
  "name": "wdio-mocha-framework",
  "version": "0.3.5",
  "description": "A WebdriverIO plugin. Adapter for Mocha testing framework.",
  "main": "build/adapter.js",
  "scripts": {
    "test": "grunt mocha_istanbul",
    "dev": "mocha --watch -r should -t 50000 --compilers js:babel/register test/*.spec.js",
    "build": "grunt build",
    "prepublish": "npm prune && npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdriverio/wdio-mocha-framework.git"
  },
  "keywords": [
    "mocha",
    "webdriverio",
    "wdio",
    "wdio-plugin",
    "wdio-framework"
  ],
  "author": "Christian Bromann <christian@saucelabs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webdriverio/wdio-mocha-framework/issues"
  },
  "homepage": "https://github.com/webdriverio/wdio-mocha-framework#readme",
  "dependencies": {
    "mocha": "^2.3.3",
    "wdio-sync": "^0.5.7",
    "babel-runtime": "^5.8.25"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.7",
    "babel-plugin-rewire": "^0.1.22",
    "codeclimate-test-reporter": "^0.1.1",
    "eslint": "^1.4.1",
    "eslint-config-standard": "^4.3.2",
    "eslint-plugin-standard": "^1.3.0",
    "grunt": "^0.4.5",
    "grunt-babel": "^5.0.1",
    "grunt-bump": "^0.6.0",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-env": "^0.4.4",
    "grunt-eslint": "^17.1.0",
    "grunt-mocha-istanbul": "^3.0.1",
    "grunt-npm": "0.0.2",
    "isparta": "^3.5.3",
    "load-grunt-tasks": "^3.2.0",
    "q": "^1.4.1",
    "should": "^7.1.0",
    "sinon": "^1.17.1"
  },
  "contributors": [
    "George Crawford <george.crawford@ft.com>",
    "Christian Bromann <github@christian-bromann.com>",
    "George Crawford <g.o.crawford@gmail.com>"
  ],
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "rewire"
        ]
      }
    }
  }
}
